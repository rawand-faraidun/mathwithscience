<%- include('../../defaults/header') %>

<%- include('../../pageTitle-backPage') %>





<!--working area-->
<section class="container">
    <!--calculator-->
    <section id="calculator" class="calculator">

        <h3 class="name">Input the Value</h3>
        <!--working part-->
        <div class="calc">
            <!--input part-->
            <div class="input">
                <label class="label">Value</label>
                <input type="number" id="value" autocomplete="off" class="field" placeholder="" oninput="ifPrime(this)">
            </div>

            <!--answer part-->
            <div class="answer">
                <div class="ans isPrime">
                    <h3 id="isPrime">Checked status</h3>
                </div>
                <div class="ans nextPrime">
                    <label class="label">Next Prime</label>
                    <input type="text" id="nextPrime" autocomplete="off" class="field" placeholder="" readonly>
                </div>
            </div>
        </div>

    </section>
</section>






<%- include('../../defaults/footer') %>