  "bugs": {
    "url": "https://github.com/Rawand-Faraidun/math-with-science/issues"
  },
  "homepage": "https://github.com/Rawand-Faraidun/math-with-science#readme",
  "dependencies": {
    "concurrently": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "mongoose": "^5.12.0",
    "react-helmet": "^6.1.0"
